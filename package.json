{
  "name": "wav-encoder",
  "description": "isomorphic wav data encoder",
  "version": "0.2.2",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/wav-encoder/issues"
  },
  "dependencies": {
    "dataview2": "^0.1.0",
    "inline-worker": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-eslint": "^3.1.15",
    "browserify": "^10.2.4",
    "eslint": "^0.22.1",
    "espower-babel": "^3.2.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.23"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/wav-encoder/",
  "keywords": [
    "encode",
    "isomorphic",
    "wav"
  ],
  "licenses": "MIT",
  "main": "./lib/index.js",
  "peerDependencies": {
    "audiodata": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/wav-encoder.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify ./lib/index.js --standalone WavEncoder --outfile build/wav-encoder.js",
    "build-minify": "uglifyjs build/wav-encoder.js -m -r self -o build/wav-encoder.min.js",
    "build-to5": "babel src --out-dir lib && cp src/EncoderWorker.js lib/",
    "cover": "babel-node $(npm bin)/isparta cover --report text --report html --report lcov _mocha",
    "lint": "eslint src test",
    "test": "mocha --compilers js:espower-babel/guess",
    "travis": "npm run lint && npm run test"
  }
}
